{
	"expo": {
		"name": "habit-builder",
		"slug": "habit-builder",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/images/icon.png",
		"scheme": "myapp",
		"userInterfaceStyle": "automatic",
		"splash": {
			"image": "./assets/images/habit-builder-splahscreen.png",
			"resizeMode": "cover",
			"backgroundColor": "#ffffff"
		},
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/images/adaptive-icon.png",
				"backgroundColor": "#ffffff"
			},
			"package": "com.roberto44.habitbuilder"
		},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./assets/images/favicon.png"
		},
		"plugins": [
			"expo-router",
			[
				"expo-font",
				{
					"fonts": ["./assets/fonts/Klasik Regular.otf"]
				}
			]
		],
		"experiments": {
			"typedRoutes": true
		}
	}
}
